public class ProcessAction {

    public ProcessAction() {

    }

    public void actionHandle(String action) {
        action = action.toLowerCase();
        switch (action) {
            case "start":
                new Start();
                break;
            case "quit":
                System.exit(0);
                break;
            default :
                System.out.println("Unknown Command");
                break;
        }
    }

